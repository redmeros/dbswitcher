<UserControl x:Class="DBSwitcher.DbConfigControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:DBSwitcher"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Name="MainPanel" Background="White">
        <GroupBox>
            <GroupBox.Header>
                Dane połączeń baz danych
            </GroupBox.Header>
            <StackPanel Name="DataSourcesStackPanel">
            </StackPanel>
        </GroupBox>
        <GroupBox>
            <GroupBox.Header>
                Dane katalogu support
            </GroupBox.Header>
            <StackPanel Orientation="Vertical" Name="DirStackPanel">
            </StackPanel>
        </GroupBox>
    </StackPanel>
</UserControl>